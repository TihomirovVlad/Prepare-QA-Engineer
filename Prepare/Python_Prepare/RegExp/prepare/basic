import re

# Символы которые ДОЛЖЕН знать:
.       # Любой символ
\d      # Цифра [0-9]
\w      # Буква/цифра/_ [a-zA-Z0-9_]
\s      # Пробельный символ (пробел, таб)
^       # Начало строки
$       # Конец строки
*       # 0 или больше раз
+       # 1 или больше раз
?       # 0 или 1 раз
{n}     # Ровно n раз
{n,m}   # От n до m раз
[abc]   # Любой из a, b, c
[^abc]  # Любой кроме a, b, c
|       # ИЛИ
()      # Группа